{
  "name": "ttv-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma migrate dev && . ./scripts/dev.sh",
    "dev:postgres": "prisma migrate dev --schema prisma/schema.postgres.prisma && . ./scripts/dev.sh",
    "dev-server": "next dev",
    "build": "next build",
    "build:postgres": "prisma migrate deploy --schema prisma/schema.postgres.prisma && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "migrate-dev": "prisma migrate dev"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@prisma/client": "^5.4.1",
    "autoprefixer": "10.4.16",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.2",
    "jotai": "^2.4.2",
    "js-sha256": "^0.10.1",
    "next": "^14.2.5",
    "next-auth": "^4.24.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "3.0.3",
    "prisma": "^5.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
